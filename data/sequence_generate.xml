<odoo>
    <data noupdate="1">

        <record id="seq_room" model="ir.sequence">
            <field name="name">Rooms Sequence</field>
            <field name="code">hotel.room</field>
            <field name="prefix">R/%(day)s/%(month)s/%(year)s/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
        </record>

        <record id="seq_guest" model="ir.sequence">
            <field name="name">Guest Sequence</field>
            <field name="code">hotel.guest</field>
            <field name="prefix">G/%(day)s/%(month)s/%(year)s/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
        </record>

        <record id="seq_booking" model="ir.sequence">
            <field name="name">Booking Sequence</field>
            <field name="code">room.booking</field>
            <field name="prefix">B/%(day)s/%(month)s/%(year)s/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
        </record>

        <record id="seq_hotel_facility_ticket" model="ir.sequence">
            <field name="name">Hotel Facility Ticket</field>
            <field name="code">hotel.facility.ticket</field>
            <field name="prefix">F/%(day)s/%(month)s/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
